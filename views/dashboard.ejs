    <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
    <script src="/js/rellax.min.js"></script>
    <script src="/js/main.js"></script>
    <script>
        //Initialize AOS
        AOS.init();
        //Initialize Rellax.js
        var rellax = new Rellax('.rellax'); 
    </script>
