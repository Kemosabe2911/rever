                <li><a href="/contact">Contact</a></li>
            </ul>
        </div>
        <div class="footer-content">
            Copyright &copy; 2021 Kemosabe2911
        </div>
    </footer>
	<nav>
		<h2>Logo</h2>
		<div class="close">
			<div></div>
		</div>
		<ul>
			<li><a href="/home">Home</a></li>
			<li><a href="/about">About</a></li>
			<li><a href="/dashboard">Dashboard</a></li>
			<li><a href="/contact">Contact</a></li>
		</ul>
	</nav>
